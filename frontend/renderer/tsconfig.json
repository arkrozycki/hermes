{
  "extends": "../tsconfig.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "../dist/types/**/*.ts",
    "types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strictNullChecks": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  }
}
